{
  "name": "@visionappscz/stylelint-config-visionapps",
  "version": "2.0.1",
  "description": "VisionApps' shareable config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "visionapps"
  ],
  "author": "visionappscz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/visionappscz/stylelint-config-visionapps.git"
  },
  "bugs": {
    "url": "https://github.com/visionappscz/stylelint-config-visionapps/issues"
  },
  "homepage": "https://github.com/visionappscz/stylelint-config-visionapps#readme",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint --config index.js __tests__/css-valid.css",
    "lint:js": "eslint . --ignore-path .gitignore",
    "pretest": "npm run lint",
    "test": "jest",
    "watch": "jest --watch"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  },
  "dependencies": {
    "stylelint-config-standard": "^24.0.0"
  },
  "devDependencies": {
    "@visionappscz/eslint-config-visionapps": "^1.5.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.1",
    "stylelint": "^14.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@visionappscz/eslint-config-visionapps"
    ],
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
